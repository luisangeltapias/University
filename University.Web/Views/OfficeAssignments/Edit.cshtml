@model University.BL.DTOs.OfficeAssignmentDTOs
@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>
@Html.ValidationSummary(false, "", new { @class = "text-danger" })
<form action="/OfficeAssignments/Edit" method="post">
    @Html.HiddenFor(m => m.InstructorID)

    @Html.DisplayNameFor(m => m.Location)
    @Html.EditorFor(m => m.Location, new { htmlAttributes = new { @class = "form-control" } })
    @Html.ValidationMessageFor(m => m.Location, "", new { @class = "text-danger" })

    <input type="submit" value="Edit" class="btn btn-primary" />
   
</form>
